import hudson.model.*
import hudson.EnvVars
import java.net.URL
 

    try {
      node { 
	mytools = load 'Tools.groovy'
     stage("\u2776 Build Stage") { 
          echo "\u2600 BUILD_URL=${env.BUILD_URL}"
		  mytools.hello()
        }
      }
	} // try end
	catch (exception) {

	} finally {
		echo "FINALLY!!"	  
	}